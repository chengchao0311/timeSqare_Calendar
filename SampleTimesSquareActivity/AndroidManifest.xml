<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.squareup.timessquare.sample"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="16" />
    
    <uses-permission android:name="android.permission.WRITE_CALENDAR"/>
    <uses-permission android:name="android.permission.READ_CALENDAR"/>
    
    <application
        android:label="TimesSquare Sample"
        android:icon="@drawable/icon"
        android:debuggable="true"
        >

        <activity
            android:name=".SampleTimesSquareActivity"
            android:label="Calendar Picker Sample"
            android:configChanges="orientation|screenSize|keyboardHidden"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        
        <activity android:name="com.squareup.timessquare.sample.EventDetail">
        </activity>
        <activity android:name="ChooseDate">
        </activity>
        
    </application>
</manifest>
